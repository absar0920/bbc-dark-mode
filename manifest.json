{
  "mainfest_verison": 3,
  "name": "BBC Dark Mode",
  "version": 1.0,
  "description": "This chrome extension changes the BBC website to dark mode",
  "content_scripts": [
    {
      "matches": ["*://*.bbc.co.uk/*"],
      "js": ["contentscript.js"],
      "css": ["contentstyles.css"],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}
